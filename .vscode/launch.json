{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Django",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/eddai_EliteDangerousApiInterface/manage.py",
      "args": [
        "runserver",
        "--settings",
        "eddai_EliteDangerousApiInterface.settings.dev"
      ],
      "django": true
    },
    {
      "name": "Python: Celery Worker",
      "type": "python",
      "request": "launch",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/eddai_EliteDangerousApiInterface/",
      "module": "celery",
      "args": [
        "-A", "eddai_EliteDangerousApiInterface",
        "worker", 
        "-l","Debug", 
        "-P", "gevent", 
        "-c", "100",
        "-n", "worker1@%h",
      ],
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
    },
    {
      "name": "Python: Celery Worker EDDN",
      "type": "python",
      "request": "launch",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/eddai_EliteDangerousApiInterface/",
      "module": "celery",
      "args": [
        "-A", "eddai_EliteDangerousApiInterface",
        "worker", 
        "-l","Debug", 
        "-P", "gevent", 
        "-c", "100",
        "-n", "workerEDDN@%h",
      ],
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
    },
    {
      "name": "Python: Django Shell",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/eddai_EliteDangerousApiInterface/manage.py",
      "args": [
        "shell",
        "--settings",
        "eddai_EliteDangerousApiInterface.settings.dev"
      ],
      "django": true
    },

    {
      "name": "Python: Migrate",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/eddai_EliteDangerousApiInterface/migrate.py",
      "args": ["--settings", "eddai_EliteDangerousApiInterface.settings.dev"]
    }
  ],
  "compounds": [
    {
      "name": "Django/Celery Worker",
      "configurations": ["Python: Django", "Python: Celery Worker", "Python: Celery Worker EDDN"],
      "stopAll": true
    }
  ]
}
